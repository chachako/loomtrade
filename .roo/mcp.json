{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "alwaysAllow": [
        "resolve-library-id",
        "get-library-docs"
      ],
      "timeout": 300
    },
    "package-version": {
      "command": "mcp-package-version"
    }
  }
}